generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url = env("DB_URL")
}

model GuildConfig {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  guildId String @unique
  greetings String[]
  maxMessagesCleared Int
  musicChannelId String
  defaultRepeatMode Int
}