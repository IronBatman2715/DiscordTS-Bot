{
  "name": "discordts-bot",
  "version": "1.0.0",
  "author": {
    "name": "IronBatman2715",
    "url": "https://github.com/IronBatman2715/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IronBatman2715/DiscordTS-Bot.git"
  },
  "bugs": {
    "url": "https://github.com/IronBatman2715/DiscordTS-Bot/issues"
  },
  "license": "GNU GPLv3",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "prebuild": "rm -rf build src/@types config.json",
    "build": "tsc -p .",
    "postbuild": "run-func build/botConfig.js getConfigFile",
    "predev": "npm run updateSchema",
    "dev": "cross-env NODE_ENV=development nodemon",
    "lint": "eslint . --ext .ts --ignore-path .gitignore",
    "updateSchema": "prisma format && prisma generate",
    "registerGlobal": "node build/registerGlobalCommands.js",
    "start": "node ."
  },
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.5.0",
    "@prisma/client": "^3.15.0",
    "ajv": "^8.11.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "discord-api-types": "^0.33.5",
    "discord-music-player": "^8.3.2",
    "discord.js": "^13.8.0",
    "dotenv": "^16.0.1",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "prisma": "^3.15.0",
    "run-func": "^3.0.0",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "engines": {
    "node": ">=16.9.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "schema": "src/database/schema.prisma"
  }
}
