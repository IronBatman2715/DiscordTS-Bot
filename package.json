{
  "name": "discordts-bot",
  "version": "1.0.0",
  "description": "A customizable Discord bot based on discord.js v14 with Typescript!",
  "author": {
    "name": "IronBatman2715",
    "url": "https://github.com/IronBatman2715/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IronBatman2715/DiscordTS-Bot.git"
  },
  "bugs": {
    "url": "https://github.com/IronBatman2715/DiscordTS-Bot/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "build": "tsc",
    "postbuild": "run-func build/botConfig.js getConfigFile true",
    "predev": "prisma format && prisma generate",
    "dev": "cross-env NODE_ENV=development nodemon",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "registerGlobal": "node build/registerGlobalCommands.js",
    "start": "node ."
  },
  "dependencies": {
    "@discordjs/rest": "^1.0.1",
    "@prisma/client": "^4.1.1",
    "ajv": "^8.11.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "discord-api-types": "^0.37.1",
    "discord-music-player": "^9.0.2",
    "discord.js": "^14.1.2",
    "dotenv": "^16.0.1",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/node": "^18.6.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prisma": "^4.1.1",
    "run-func": "^3.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16.9.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "schema": "src/database/schema.prisma"
  }
}
