{
  "name": "discordts-bot",
  "version": "1.0.0",
  "description": "A customizable Discord bot based on discord.js v14 with Typescript!",
  "author": {
    "name": "IronBatman2715",
    "url": "https://github.com/IronBatman2715/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IronBatman2715/DiscordTS-Bot.git"
  },
  "bugs": {
    "url": "https://github.com/IronBatman2715/DiscordTS-Bot/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "postinstall": "prisma generate --no-hints",
    "build": "tsc",
    "postbuild": "run-func build/botConfig.js getConfigFile true",
    "predev": "prisma format && prisma generate --no-hints",
    "dev": "nodemon",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "commands:register": "node build/scripts/discordAPI/registerCommands.js",
    "commands:resetDev": "cross-env NODE_ENV=development ts-node --files src/scripts/discordAPI/resetCommands.ts --enable-source-maps",
    "commands:resetProd": "node build/scripts/discordAPI/resetCommands.js",
    "start": "node ."
  },
  "dependencies": {
    "@discord-player/extractor": "^4.5.1",
    "@prisma/client": "^5.18.0",
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "discord-player": "^6.7.1",
    "discord-player-youtubei": "^1.2.6",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "lodash": "^4.17.21",
    "mediaplex": "^0.0.9",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.11",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prisma": "^5.18.0",
    "run-func": "^3.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4"
  },
  "overrides": {
    "eslint": {
      "file-entry-cache": "8.x"
    }
  },
  "engines": {
    "node": ">=18.18"
  }
}
