{
  "name": "discordts-bot",
  "version": "1.0.0",
  "description": "A customizable Discord bot based on discord.js v14 with Typescript!",
  "author": {
    "name": "IronBatman2715",
    "url": "https://github.com/IronBatman2715/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IronBatman2715/DiscordTS-Bot.git"
  },
  "bugs": {
    "url": "https://github.com/IronBatman2715/DiscordTS-Bot/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "build": "tsc",
    "postbuild": "run-func build/botConfig.js getConfigFile true",
    "predev": "prisma format && prisma generate",
    "dev": "cross-env NODE_ENV=development nodemon",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "registerGlobal": "node build/registerGlobalCommands.js",
    "start": "node ."
  },
  "dependencies": {
    "@discordjs/rest": "^1.7.0",
    "@prisma/client": "^4.13.0",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "discord-api-types": "^0.37.40",
    "discord-music-player": "^9.1.1",
    "discord.js": "^14.9.0",
    "dotenv": "^16.0.3",
    "isomorphic-unfetch": "^4.0.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "^18.16.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "prisma": "^4.13.0",
    "run-func": "^3.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.9.0"
  }
}
