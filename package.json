{
  "name": "discordts-bot",
  "author": {
    "name": "IronBatman2715",
    "url": "https://github.com/IronBatman2715/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IronBatman2715/DiscordTS-Bot.git"
  },
  "bugs": {
    "url": "https://github.com/IronBatman2715/DiscordTS-Bot/issues"
  },
  "license": "GNU GPLv3",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "prebuild": "rm -rf build src/@types",
    "build": "tsc -p .",
    "predev": "npm run updateSchema",
    "dev": "cross-env NODE_ENV=development nodemon",
    "lint": "eslint . --ext .ts --ignore-path .gitignore",
    "updateSchema": "prisma format && prisma generate",
    "viewDB": "prisma studio",
    "registerGlobal": "cross-env NODE_ENV=development ts-node src/registerGlobalCommands.ts --enable-source-maps",
    "start": "node ."
  },
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordjs/rest": "^0.4.1",
    "@prisma/client": "^3.14.0",
    "discord-api-types": "^0.33.2",
    "discord-music-player": "^8.3.2",
    "discord.js": "^13.7.0",
    "dotenv": "^16.0.1",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "ts-node": "^10.8.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  },
  "engines": {
    "node": ">=16.6.0"
  },
  "prisma": {
    "schema": "src/database/schema.prisma"
  }
}
