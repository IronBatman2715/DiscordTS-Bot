{
  "name": "discordts-bot",
  "version": "1.0.0",
  "description": "A customizable Discord bot based on discord.js v14 with Typescript!",
  "author": {
    "name": "IronBatman2715",
    "url": "https://github.com/IronBatman2715/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IronBatman2715/DiscordTS-Bot.git"
  },
  "bugs": {
    "url": "https://github.com/IronBatman2715/DiscordTS-Bot/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "main": "build/index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "build": "tsc",
    "postbuild": "run-func build/botConfig.js getConfigFile true",
    "predev": "prisma format && prisma generate",
    "dev": "nodemon",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "registerGlobal": "node build/registerGlobalCommands.js",
    "start": "node ."
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "discord-music-player": "^9.1.1",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "isomorphic-unfetch": "^4.0.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "prisma": "^5.3.1",
    "run-func": "^3.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}
